{
  "name": "cfc-website",
  "version": "1.0.0",
  "description": "CFC / chess.ca website",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "hugo:version": "cd ../hugo && hugo version",
    "hugo:build": "cd ../hugo && hugo --cleanDestinationDir",
    "hugo:serve": "cd ../hugo && hugo server",
    "hugo:serve+drafts": "cd ../hugo && hugo server --buildDrafts",
    "hugo:new-post": "cd ../hugo && hugo",
    "rollup:watch-dev": "rollup -c rollup.config.js --environment BUILD:dev --watch",
    "rollup:build-dev": "rollup -c rollup.config.js --environment BUILD:dev",
    "rollup:build-prod": "rollup -c rollup.config.js --environment BUILD:prod"
  },
  "author": "Don Parakin",
  "license": "COPYRIGHT",
  "hugo-bin": {
    "buildTags": "extended"
  },
  "dependencies": {
    "hugo-bin": "*",
    "bulma": "*",
    "alpinejs": "*",
    "gsap": "*",
    "anchor-js": "*",
    "svelte": "*",
    "rollup": "*",
    "@rollup/plugin-node-resolve": "*",
    "rollup-plugin-delete": "*",
    "rollup-plugin-svelte": "*",
    "rollup-plugin-sizes": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/parakin/cfc-website.git"
  }
}
